            <spring value="0.2 0.2 0.2"/>
            <summer value="0.2 0.2 0.2"/>
            <autumn value="0.2 0.2 0.2"/>
            <winter value="0.2 0.2 0.2"/>
            <snow   value="0.2 0.2 0.2"/>
        </envAlbedoGroundColors>

        <!--
            When used to convert with the daylight system:

            6 => nightEnd
            7 => dayStart
            19 => dayEnd
            20 => nightStart
        -->

        <colorGrading>
            <key timeHours="0"  filename="$data/maps/mapUS/config/colorGradingNight.xml" />
            <key timeHours="5"  filename="$data/maps/mapUS/config/colorGradingNight.xml" />
            <key timeHours="6"  filename="$data/maps/mapUS/config/colorGrading.xml" />
            <key timeHours="20" filename="$data/maps/mapUS/config/colorGrading.xml" />
            <key timeHours="22" filename="$data/maps/mapUS/config/colorGradingNight.xml" />
            <key timeHours="24" filename="$data/maps/mapUS/config/colorGradingNight.xml" />
        </colorGrading>

        <autoExposure>
            <!-- key luminance, minExposure, maxExposure -->
            <key value="0.15 -0.5 1.25" time="0" />
            <key value="0.15 -1.0 1.5"  time="5" />
            <key value="0.17 -0.5 0.65" time="8" />
            <key value="0.19 -1.0 0.85" time="18" />
            <key value="0.15 -1.0 1.5"  time="21" />
            <key value="0.15 -0.5 1.25" time="24" />
        </autoExposure>

        <fixedExposure>
            <key value="0.5" time="0" />
            <key value="0.8" time="6" />
            <key value="0.9" time="8" />
            <key value="1.3" time="12" />
            <key value="0.8" time="18" />
            <key value="0.5" time="20" />
        </fixedExposure>

        <sunRotation heightAngleLimitRotation="60" heightAngleLimitRotationStart="56" heightAngleLimitRotationEnd="80">
            <key value="0"   time="0" />
            <key value="80"  time="6.5" />
            <key value="-80" time="6.5" /> <!-- sunrise 6 -->
            <key value="0"   time="12" />
            <key value="80"  time="19.5" />
            <key value="-80" time="19.5" /> <!-- sunset 20 -->
            <key value="0"   time="24" />
        </sunRotation>

        <lightScatteringRotation>
            <!-- primary and secondary -->
            <key value="0 -180"   time="0" />
            <key value="80 -80"    time="6.5" /> <!-- switch, sunrise ~6 -->
            <key value="-80 80"     time="6.5" />
            <key value="0 180"    time="12" />
            <key value="0 -180"   time="12" />
            <key value="80 -80"    time="19.5" /> <!-- switch, sunset ~20 -->
            <key value="-80 80"     time="19.5" />
            <key value="0 180"    time="24" />
        </lightScatteringRotation>

        <primaryDynamicLightingScale>
            <key value="1" time="0" />
            <key value="1" time="6.2" />
            <key value="0.0" time="6.5" />  <!-- sunrise switch -->
            <key value="1" time="6.8" />
            <key value="1" time="19.2" />
            <key value="0.0" time="19.5" /> <!-- sunset switch -->
            <key value="1" time="19.8" />
            <key value="1" time="24" />
        </primaryDynamicLightingScale>

        <primaryExtraterrestrialColor>
            <key value="0.04 0.07 0.11" time="0" />
            <key value="0.04 0.07 0.11" time="6.5" />
            <key value="1.0 1.0 1.0"    time="6.5" />  <!-- sunrise switch -->
            <key value="1.0 1.0 1.0"    time="19.5" /> <!-- sunset switch -->
            <key value="0.04 0.07 0.11" time="19.5" />
            <key value="0.04 0.07 0.11" time="24" />
        </primaryExtraterrestrialColor>

        <secondaryExtraterrestrialColor>
            <key value="0.0 0.0 0.0"    time="0" />
            <key value="0.0 0.0 0.0"    time="5" />
            <key value="1.0 1.0 1.0"    time="6.5" />
            <key value="0.04 0.07 0.11" time="6.5" />  <!-- sunrise switch (secondary becomes moon) -->
            <key value="0.0 0.0 0.0"    time="7" />
            <key value="0.0 0.0 0.0"    time="18.5" />
            <key value="0.04 0.07 0.11" time="19.5" /> <!-- sunset switch (secondary becomes sun) -->
            <key value="1.0 1.0 1.0"    time="19.5" />
            <key value="0.0 0.0 0.0"    time="21" />
            <key value="0.0 0.0 0.0"    time="24" />
